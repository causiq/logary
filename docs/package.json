  "devDependencies": {
    "@babel/node": "^7.2.2",
    "@types/react": "^16.9.19",
    "axios": "^0.19.2",
    "concurrently": "^5.1.0",
    "cypress": "^4.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "htmlparser2": "^4.0.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "next build",
    "build:search": "babel-node ./public/search/buildIndex.js",
    "build:search:auto": "concurrently --kill-others \"yarn run build:search\" \"yarn run dev\"",
    "build:image": "docker build . -t haaf/logary-docs:$(git rev-parse HEAD)",
    "dev": "COMMIT_SHA=$(git rev-parse HEAD) COMMIT_REF=$(git symbolic-ref HEAD) next dev",
    "start": "COMMIT_SHA=$(git rev-parse HEAD) COMMIT_REF=$(git symbolic-ref HEAD) next start --port 80",
    "cypress:open": "cypress open",
    "cypress:run": "now-pipeline --as CYPRESS_baseUrl --alias logary.tech --test 'cypress run --record --key 714cc2a7-fdda-4f31-85da-844d60a84e1c'"
  }
}
