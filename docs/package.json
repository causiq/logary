    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-plugin-macros": "^2.5.1",
    "babel-plugin-preval": "^4.0.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "email-addresses": "^3.0.3",
    "highlight.js": "^9.15.6",
    "isomorphic-unfetch": "^3.0.0",
    "logary": "^5.0.2",
    "lunr": "^2.3.6",
